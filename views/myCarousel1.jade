#myCarousel1
    .container
        br
        #myCarousel.carousel.slide(data-ride='carousel')
            // Indicators
            ol.carousel-indicators
                li(data-target='#myCarousel', data-slide-to='0')
                li(data-target='#myCarousel', data-slide-to='1')
                li.active(data-target='#myCarousel', data-slide-to='2')
                li(data-target='#myCarousel', data-slide-to='3')
                li(data-target='#myCarousel', data-slide-to='4')
            // Wrapper for slides
            .carousel-inner(role='listbox')
                .item
                    img(src='images/house1.jpg', alt='house')
                    .carousel-caption
                        h3 Amazing architecture
                .item
                    img(src='images/house3.jpg', alt='Unbelievable cottage')
                    .carousel-caption
                        h3 Unbelievable cottage
                .item.active
                    img(src='images/waterHouse.jpg', alt='Water House')
                    .carousel-caption
                        h3 The flutter House
                .item
                    img(src='images/house2.jpg', alt='Luxury House')
                    .carousel-caption
                        h3 Luxury House
                .item
                    img(src='images/house4.jpg', alt='Cubic House')
                    .carousel-caption
                        h3 The cubic House

            // Left and right controls
            a.left.carousel-control(href='#myCarousel', role='button', data-slide='prev')
                span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
                span.sr-only Previous
            a.right.carousel-control(href='#myCarousel', role='button', data-slide='next')
                span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
                span.sr-only Next
